import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int base=sc.nextInt();
        int ses=sc.nextInt();
        int perses=sc.nextInt();
        int bonus=sc.nextInt();
        int main=sc.nextInt();
        int mul=ses*perses;
        int add=mul+base+bonus;
        int sub=add-main;
       
        System.out.println(sub);
    }
}
